{
  "name": "movie-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build-admin": "cd admin-frontend && npm run build",
    "dev-admin": "concurrently \"npm run dev\" \"cd admin-frontend && npm run dev\"",
    "start-with-admin": "npm run build-admin && npm start",
    "create-admin": "node scripts/create-admin.js",
    "seed": "node scripts/seedTestData.js",
    "clean": "node scripts/cleanTestData.js",
    "reseed": "npm run clean && npm run seed",
    "generate-test": "node scripts/generate-test-data.js",
    "clean-test": "node scripts/clean-test-data.js",
    "regenerate-test": "npm run clean-test && npm run generate-test",
    "test-interactions": "node scripts/test-user-interactions.js",
    "fix-series": "node scripts/fix-series-episodes.js",
    "validate-data": "node scripts/fix-series-episodes.js",
    "fix-swagger": "node scripts/fix-swagger.js"
  },
  "dependencies": {
    "@payos/node": "^1.0.10",
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.1.0",
    "mongoose": "^8.15.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.1.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "axios": "^1.10.0",
    "concurrently": "^9.2.0"
  }
}
